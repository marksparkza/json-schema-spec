{
    "$schema": "https://json-schema.org/draft/next/schema",
    "$id": "https://json-schema.org/draft/next/meta/meta-data",
    "$vocabulary": {
        "https://json-schema.org/draft/next/vocab/meta-data": true
    },
    "$dynamicAnchor": "meta",

    "title": "Meta-data vocabulary meta-schema",

    "type": ["object", "boolean"],
    "properties": {
        "title": {
            "type": "string",
            "$annotation": true
        },
        "description": {
            "type": "string",
            "$annotation": true
        },
        "default": {
            "$annotation": true
        },
        "deprecated": {
            "type": "boolean",
            "default": false,
            "$annotation": true
        },
        "readOnly": {
            "type": "boolean",
            "default": false,
            "$annotation": true
        },
        "writeOnly": {
            "type": "boolean",
            "default": false,
            "$annotation": true
        },
        "examples": {
            "type": "array",
            "items": true,
            "$annotation": true
        }
    }
}
