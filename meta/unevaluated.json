{
    "$schema": "https://json-schema.org/draft/next/schema",
    "$id": "https://json-schema.org/draft/next/meta/unevaluated",
    "$vocabulary": {
        "https://json-schema.org/draft/next/vocab/unevaluated": true
    },
    "$dynamicAnchor": "meta",

    "title": "Unevaluated applicator vocabulary meta-schema",
    "type": ["object", "boolean"],
    "properties": {
        "unevaluatedItems": {
            "$dynamicRef": "#meta",
            "$annotation": { "const": true },
            "$error": { "$ref": "#/$defs/nonNegativeIntegerArray" }
        },
        "unevaluatedProperties": {
            "$dynamicRef": "#meta",
            "$annotation": { "$ref": "#/$defs/stringArray" },
            "$error": { "$ref": "#/$defs/stringArray" }
        }
    }
}
